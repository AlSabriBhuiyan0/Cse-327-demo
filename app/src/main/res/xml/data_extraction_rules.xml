<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude model files from cloud backup -->
        <exclude domain="file" path="models/"/>
    </cloud-backup>
    <device-transfer>
        <!-- Exclude model files from device transfer -->
        <exclude domain="file" path="models/"/>
    </device-transfer>
</data-extraction-rules> 